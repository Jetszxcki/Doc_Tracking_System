Create database named document_tracking_system

For Admin Table
CREATE TABLE admin (
  username VARCHAR(40) NOT NULL, 
  password VARCHAR(40) NOT NULL,
);

For User Table
CREATE TABLE users ( 
  userID int(9) NOT NULL auto_increment, 
  fullname VARCHAR(50) NOT NULL,
  username VARCHAR(40) NOT NULL,
  email VARCHAR(40) NOT NULL,
  password VARCHAR(40) NOT NULL, 
  PRIMARY KEY(userID) 
);
